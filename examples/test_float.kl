// test_float.kl - Test floating-point operations for native compilation

fn main() -> i32 {
    let a: f64 = 3.5
    let b: f64 = 2.0

    let sum: f64 = a + b        // 5.5
    let diff: f64 = a - b       // 1.5
    let prod: f64 = a * b       // 7.0
    let quot: f64 = a / b       // 1.75

    // Can't return float from main() yet, so return integer
    // Just verify it compiles and runs
    return 42
}
