// fibonacci.kl - Fibonacci sequence in Klar

fn fib(n: i32) -> i32 {
    if n <= 1 {
        return n
    } else {
        return fib(n - 1) + fib(n - 2)
    }
}

fn main() -> i32 {
    println("Fibonacci sequence:")
    let result: i32 = fib(10)
    println("Result computed")
    return result - 55  // fib(10) = 55, so returns 0
}
