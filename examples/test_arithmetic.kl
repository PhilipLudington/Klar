// test_arithmetic.kl - Test arithmetic operations for native compilation
// Tests: add, sub, mul, div, mod, wrapping, comparisons

fn main() -> i32 {
    // Basic arithmetic
    let a = 10
    let b = 3

    let sum = a + b          // 13
    let diff = a - b         // 7
    let prod = a * b         // 30
    let quot = a / b         // 3
    let rem = a % b          // 1

    // Chain operations
    let x = 10 + 20 * 3      // 70 (20*3=60, 60+10=70)
    let y = x / 2            // 35

    // Return result for verification
    y
}
