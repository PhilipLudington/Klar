// test_arithmetic.kl - Test arithmetic operations for native compilation
// Tests: add, sub, mul, div, mod, wrapping, comparisons

fn main() -> i32 {
    // Basic arithmetic
    let a: i32 = 10
    let b: i32 = 3

    let sum: i32 = a + b          // 13
    let diff: i32 = a - b         // 7
    let prod: i32 = a * b         // 30
    let quot: i32 = a / b         // 3
    let rem: i32 = a % b          // 1

    // Chain operations
    let x: i32 = 10 + 20 * 3      // 70 (20*3=60, 60+10=70)
    let y: i32 = x / 2            // 35

    // Return result for verification
    return y
}
