// Test Into[T] trait for type conversions

struct Celsius { value: f64 }
struct Fahrenheit { value: f64 }

impl Fahrenheit {
    fn get_value(self: Fahrenheit) -> f64 {
        return self.value
    }
}

impl Celsius: Into[Fahrenheit] {
    fn into(self: Celsius) -> Fahrenheit {
        return Fahrenheit { value: self.value * 1.8 + 32.0 }
    }
}

fn main() -> i32 {
    let c: Celsius = Celsius { value: 100.0 }
    let f: Fahrenheit = c.into()

    // 100 C = 212 F
    let temp: f64 = f.get_value()
    if temp > 211.9 and temp < 212.1 {
        return 0
    }
    return 1
}
