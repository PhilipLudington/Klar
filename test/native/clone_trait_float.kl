// Test for builtin Clone trait - float

fn main() -> i32 {
    // Test float cloning
    let a: f64 = 3.14
    let b: f64 = a.clone()

    // b should be a copy of a with the same value
    if a.eq(b) {
        // Values are equal - correct!
        return 0
    } else {
        // Values differ - should not happen
        return 1
    }
}
