// Test for generic struct methods (impl blocks on generic structs)

struct Pair[T] {
    first: T,
    second: T,
}

impl[T] Pair[T] {
    fn get_first(self: Pair[T]) -> T {
        self.first
    }

    fn get_second(self: Pair[T]) -> T {
        self.second
    }
}

fn main() -> i32 {
    let p = Pair[i32] { first: 10, second: 32 }
    p.get_first() + p.get_second()
}
