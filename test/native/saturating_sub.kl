// Test saturating subtraction (-|)
fn main() -> i32 {
    // Saturate at i32 min: -2147483647 -| 2 saturates at min
    let near_min: i32 = -2147483647
    let a: i32 = near_min -| 2
    // Result must be <= near_min (saturated to INT_MIN)
    if a > near_min {
        return 1
    }

    // Saturate at i32 max: 2147483647 -| -1 = 2147483647
    let max: i32 = 2147483647
    let b: i32 = max -| -1
    if b != max {
        return 2
    }

    // Normal case: no saturation
    let c: i32 = 30 -| 10
    if c != 20 {
        return 3
    }

    return 0
}
