// Test function with many parameters (more than register args)
// On System V AMD64: 6 integer registers (rdi, rsi, rdx, rcx, r8, r9)
// Parameters 7+ go on the stack

fn add_many(a: i32, b: i32, c: i32, d: i32, e: i32, f: i32, g: i32, h: i32) -> i32 {
    return a + b + c + d + e + f + g + h
}

fn main() -> i32 {
    // 1 + 2 + 3 + 4 + 5 + 6 + 7 + 8 = 36
    return add_many(1, 2, 3, 4, 5, 6, 7, 8)
}
