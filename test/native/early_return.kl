// Test early return handling

fn abs(x: i32) -> i32 {
    if x < 0 {
        return -x
    }
    x
}

fn factorial(n: i32) -> i32 {
    if n <= 1 {
        return 1
    }
    n * factorial(n - 1)
}

fn main() -> i32 {
    let a = abs(-42)       // 42
    let b = factorial(5)   // 120
    // Return sum as simple verification (42 + 120 = 162)
    a + b
}
