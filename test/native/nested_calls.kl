// Test nested function calls

fn double(x: i32) -> i32 {
    x * 2
}

fn add(a: i32, b: i32) -> i32 {
    a + b
}

fn complex(a: i32, b: i32, c: i32) -> i32 {
    add(double(a), add(b, c))
}

fn main() -> i32 {
    // complex(5, 3, 2) = add(double(5), add(3, 2)) = add(10, 5) = 15
    complex(5, 3, 2)
}
