// Test passing a closure with captures as a function argument
// Expected exit code: 50 (apply_twice(multiply_by_5, 2) = 5 * (5 * 2) = 50)

// A higher-order function that applies a closure twice
fn apply_twice(f: fn(i32) -> i32, x: i32) -> i32 {
    return f(f(x))
}

fn main() -> i32 {
    let factor: i32 = 5
    let multiply: fn(i32) -> i32 = |x: i32| -> i32 { return x * factor }
    return apply_twice(multiply, 2)
}
