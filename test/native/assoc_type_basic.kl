// Test basic associated types

// Define a trait with an associated type
trait Container {
    type Item
    fn size(self: Self) -> i32
}

// Define a simple struct
struct IntBox {
    value: i32
}

// Implement Container for IntBox with associated type binding
impl IntBox: Container {
    type Item = i32

    fn size(self: IntBox) -> i32 {
        return 1
    }
}

fn main() -> i32 {
    let box: IntBox = IntBox { value: 42 }
    let length: i32 = box.size()

    if length == 1 {
        print("Associated type test passed!")
        return 0
    }

    print("Test failed")
    return 1
}
