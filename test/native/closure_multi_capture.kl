// Test closure that captures multiple variables
// Expected exit code: 32 (2 * 10 + 3 * 4)

fn main() -> i32 {
    let a: i32 = 2
    let b: i32 = 10
    let c: i32 = 3
    let d: i32 = 4

    // Closure that captures multiple variables
    let calc: fn(i32) -> i32 = |x: i32| -> i32 { return a * b + c * d }

    // Call the closure (x is unused, just testing capture)
    return calc(0)
}
