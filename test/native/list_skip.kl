// Test List.skip() iterator adapter

fn main() -> i32 {
    // Test 1: skip from non-empty list
    var nums: List[i32] = List.new[i32]()
    nums.push(1)
    nums.push(2)
    nums.push(3)
    nums.push(4)
    nums.push(5)

    let skipped: List[i32] = nums.skip(2)
    assert_eq(skipped.len(), 3)
    assert_eq(skipped.get(0) ?? -1, 3)
    assert_eq(skipped.get(1) ?? -1, 4)
    assert_eq(skipped.get(2) ?? -1, 5)

    // Test 2: skip(0) returns all elements
    let none_skipped: List[i32] = nums.skip(0)
    assert_eq(none_skipped.len(), 5)
    assert_eq(none_skipped.get(0) ?? -1, 1)

    // Test 3: skip more than length returns empty list
    let all_skipped: List[i32] = nums.skip(10)
    assert_eq(all_skipped.len(), 0)

    // Test 4: skip from empty list
    var empty_list: List[i32] = List.new[i32]()
    let from_empty: List[i32] = empty_list.skip(5)
    assert_eq(from_empty.len(), 0)

    // Test 5: original list is unchanged
    assert_eq(nums.len(), 5)

    print("All list.skip() tests passed!")
    return 0
}
