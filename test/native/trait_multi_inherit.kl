// Test for multiple trait inheritance (trait A: B + C syntax)

trait Addable {
    fn add_value(self: Self) -> i32
}

trait Subtractable {
    fn sub_value(self: Self) -> i32
}

trait Calculator: Addable + Subtractable {
    fn calc(self: Self) -> i32
}

struct Numbers {
    a: i32,
    b: i32,
}

// Must implement all methods from Calculator, Addable, AND Subtractable
impl Numbers: Calculator {
    fn add_value(self: Numbers) -> i32 {
        self.a + self.b
    }

    fn sub_value(self: Numbers) -> i32 {
        self.a - self.b
    }

    fn calc(self: Numbers) -> i32 {
        self.add_value() + self.sub_value()
    }
}

fn main() -> i32 {
    let n = Numbers { a: 30, b: 12 }
    n.calc()
}
