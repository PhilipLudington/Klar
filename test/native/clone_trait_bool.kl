// Test for builtin Clone trait - bool

fn main() -> i32 {
    // Test bool cloning
    let t: bool = true
    let t2: bool = t.clone()

    let f: bool = false
    let f2: bool = f.clone()

    // Clones should equal originals
    if t.eq(t2) {
        if f.eq(f2) {
            // Both correct!
            return 0
        } else {
            // false clone failed
            return 2
        }
    } else {
        // true clone failed
        return 1
    }
}
