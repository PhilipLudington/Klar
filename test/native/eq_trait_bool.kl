// Test Eq trait for boolean values

fn main() -> i32 {
    let t: bool = true
    let f: bool = false
    let t2: bool = true

    // Test eq() on booleans
    if t.eq(t2) {
        if t.eq(f) {
            // true == false should be false
            return 1
        } else {
            // true == true is true, true == false is false - correct!
            return 0
        }
    } else {
        // true == true should be true
        return 2
    }
}
