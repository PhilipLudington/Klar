// Test for builtin Ordered trait with integers

fn main() -> i32 {
    // Test integer ordering
    let a: i32 = 10
    let b: i32 = 20
    let c: i32 = 10

    // Test lt() - less than
    let lt1: bool = a.lt(b)  // 10 < 20 = true
    let lt2: bool = b.lt(a)  // 20 < 10 = false
    let lt3: bool = a.lt(c)  // 10 < 10 = false

    // Test le() - less than or equal
    let le1: bool = a.le(b)  // 10 <= 20 = true
    let le2: bool = a.le(c)  // 10 <= 10 = true
    let le3: bool = b.le(a)  // 20 <= 10 = false

    // Test gt() - greater than
    let gt1: bool = b.gt(a)  // 20 > 10 = true
    let gt2: bool = a.gt(b)  // 10 > 20 = false
    let gt3: bool = a.gt(c)  // 10 > 10 = false

    // Test ge() - greater than or equal
    let ge1: bool = b.ge(a)  // 20 >= 10 = true
    let ge2: bool = a.ge(c)  // 10 >= 10 = true
    let ge3: bool = a.ge(b)  // 10 >= 20 = false

    // Verify all results - expect: lt1, not lt2, not lt3, le1, le2, not le3, gt1, not gt2, not gt3, ge1, ge2, not ge3
    if not lt1 { return 12 }
    if lt2 { return 1 }
    if lt3 { return 2 }
    if not le1 { return 11 }
    if not le2 { return 10 }
    if le3 { return 3 }
    if not gt1 { return 9 }
    if gt2 { return 4 }
    if gt3 { return 5 }
    if not ge1 { return 8 }
    if not ge2 { return 7 }
    if ge3 { return 6 }
    // All tests passed
    return 0
}
