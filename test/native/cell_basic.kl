// Test Cell[T] for interior mutability
// Cell provides .get() and .set() methods

fn main() -> i32 {
    // Create a Cell containing an integer
    let counter: Cell[i32] = Cell.new(10)

    // Get the initial value
    let initial: i32 = counter.get()

    // Set a new value
    counter.set(42)

    // Get the updated value
    let updated: i32 = counter.get()

    // Return the updated value (should be 42)
    return updated
}
