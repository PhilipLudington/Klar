// Test generic enum pattern matching
// Tests that match expressions with generic enum variant patterns work correctly.
// Expected exit: 100 (MyNone variant match)

enum MyOption[T] {
    MySome(T),
    MyNone,
}

fn main() -> i32 {
    // Test with None value
    let none_val: MyOption[i32] = MyOption[i32]::MyNone

    // Pattern match with generic enum type syntax
    var result1: i32 = 0
    match none_val {
        MyOption.MySome(_) => { result1 = 0 }
        MyOption.MyNone => { result1 = 100 }
    }

    return result1
}
