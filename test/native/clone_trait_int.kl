// Test for builtin Clone trait - integer

fn main() -> i32 {
    // Test integer cloning
    let a = 42
    let b = a.clone()

    // b should be a copy of a with the same value
    if a.eq(b) {
        // Values are equal - correct!
        0
    } else {
        // Values differ - should not happen
        1
    }
}
