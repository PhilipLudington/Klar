// Test for builtin Ordered trait with strings

fn main() -> i32 {
    // Test string ordering (lexicographic)
    let a: string = "apple"
    let b: string = "banana"
    let c: string = "apple"

    // Test lt() - less than
    let lt1: bool = a.lt(b)  // "apple" < "banana" = true
    let lt2: bool = b.lt(a)  // "banana" < "apple" = false

    // Test le() - less than or equal
    let le1: bool = a.le(c)  // "apple" <= "apple" = true
    let le2: bool = b.le(a)  // "banana" <= "apple" = false

    // Test gt() - greater than
    let gt1: bool = b.gt(a)  // "banana" > "apple" = true
    let gt2: bool = a.gt(b)  // "apple" > "banana" = false

    // Test ge() - greater than or equal
    let ge1: bool = a.ge(c)  // "apple" >= "apple" = true
    let ge2: bool = a.ge(b)  // "apple" >= "banana" = false

    // Verify all results
    if not lt1 { return 8 }
    if lt2 { return 1 }
    if not le1 { return 7 }
    if le2 { return 2 }
    if not gt1 { return 6 }
    if gt2 { return 3 }
    if not ge1 { return 5 }
    if ge2 { return 4 }
    // All tests passed
    return 0
}
