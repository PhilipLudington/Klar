// Test for builtin Ordered trait with strings

fn main() -> i32 {
    // Test string ordering (lexicographic)
    let a = "apple"
    let b = "banana"
    let c = "apple"

    // Test lt() - less than
    let lt1 = a.lt(b)  // "apple" < "banana" = true
    let lt2 = b.lt(a)  // "banana" < "apple" = false

    // Test le() - less than or equal
    let le1 = a.le(c)  // "apple" <= "apple" = true
    let le2 = b.le(a)  // "banana" <= "apple" = false

    // Test gt() - greater than
    let gt1 = b.gt(a)  // "banana" > "apple" = true
    let gt2 = a.gt(b)  // "apple" > "banana" = false

    // Test ge() - greater than or equal
    let ge1 = a.ge(c)  // "apple" >= "apple" = true
    let ge2 = a.ge(b)  // "apple" >= "banana" = false

    // Verify all results
    if lt1 {
        if lt2 { 1 } else {
            if le1 {
                if le2 { 2 } else {
                    if gt1 {
                        if gt2 { 3 } else {
                            if ge1 {
                                if ge2 { 4 } else {
                                    // All tests passed
                                    0
                                }
                            } else { 5 }
                        }
                    } else { 6 }
                }
            } else { 7 }
        }
    } else { 8 }
}
