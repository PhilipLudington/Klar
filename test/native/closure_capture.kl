// Test closure that captures a variable from enclosing scope
// Expected exit code: 25 (5 * 5)

fn main() -> i32 {
    let factor: i32 = 5

    // Closure that captures 'factor' from outer scope
    let multiply: fn(i32) -> i32 = |x: i32| -> i32 { return x * factor }

    // Call the closure
    return multiply(5)
}
