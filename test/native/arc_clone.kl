// Test Arc.clone() functionality
// Cloning atomically increments the reference count

fn main() -> i32 {
    // Create an Arc-wrapped integer
    let arc1 = Arc.new(42)

    // Clone the Arc (atomically increments reference count)
    let arc2 = arc1.clone()

    // Both arc1 and arc2 point to the same value
    // The value is freed when the last reference is dropped
    0
}
