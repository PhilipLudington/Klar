// test_result_is_ok.kl - Test Result.is_ok() method
// Tests: is_ok() returns true for Ok values
// Expected exit code: 1 (is_ok returns true as i1, which is 1)

fn main() -> i32 {
    let result: Result[i32, i32] = Ok(42)
    if result.is_ok() {
        return 1
    } else {
        return 0
    }
}
