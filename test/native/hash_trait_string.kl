// Test for builtin Hash trait - string

fn main() -> i32 {
    // Test string hashing
    let s1: string = "hello"
    let h1: i64 = s1.hash()
    let h2: i64 = s1.hash()

    // Hash of the same string should be consistent
    let diff1: i64 = h1 - h2
    let zero: i64 = i64.default()
    if diff1 == zero {
        // Hashes are equal - correct!
        // Also verify that different strings give different hashes
        let s2: string = "world"
        let h3: i64 = s2.hash()
        let diff2: i64 = h1 - h3
        if diff2 == zero {
            // Different strings shouldn't hash to the same value
            return 2
        }
        return 0
    } else {
        // Same string hashes differently - should not happen
        return 1
    }
}
